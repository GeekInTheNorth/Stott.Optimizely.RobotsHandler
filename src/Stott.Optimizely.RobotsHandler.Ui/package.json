{
  "name": "robots-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean-dotnet": "node -e \"var rimraf = require('rimraf'); rimraf.native('../Stott.Optimizely.RobotsHandler/Static/index*.*', { glob: true}).then(function () { console.log('files removed'); });\"",
    "copy-to-dotnet": "npx copyfiles -f ./dist/assets/**/* ../Stott.Optimizely.RobotsHandler/Static",
    "build-dotnet": "npm run clean-dotnet && npm run build && npm run copy-to-dotnet"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "vite": "^7.1.5"
  }
}